{
    "name": "vyfony/bibliography-bundle",
    "type": "symfony-bundle",
    "description": "Symfony VyfonyBibliographyBundle",
    "keywords": ["bibliography", "symfony-bundle"],
    "homepage": "https://github.com/vyfony/bibliography-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Anton Dyshkant",
            "email": "vyshkant@gmail.com",
            "homepage": "https://vyshkant.github.io"
        }
    ],
    "require": {
        "php": "^7.4",
        "doctrine/doctrine-bundle": "^2.2",
        "doctrine/orm": "^2.7",
        "sonata-project/admin-bundle": "^3.89",
        "symfony/config": "^4.4 || ^5.0",
        "symfony/console": "^4.4 || ^5.0",
        "symfony/dependency-injection": "^4.4 || ^5.0",
        "symfony/doctrine-bridge": "^4.4 || ^5.0",
        "symfony/http-kernel": "^4.4 || ^5.0",
        "symfony/yaml": "^4.4 || ^5.0"
    },
    "require-dev": {
        "enlightn/security-checker": "^1.6",
        "friendsofphp/php-cs-fixer": "^2.16",
        "phan/phan": "^2.7",
        "php-parallel-lint/php-parallel-lint": "^1.2",
        "phpro/grumphp": "^1.0",
        "phpunit/phpunit": "^7.3",
        "squizlabs/php_codesniffer": "^3.5"
    },
    "autoload": {
        "psr-4": {
            "Vyfony\\Bundle\\BibliographyBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Vyfony\\Bundle\\BibliographyBundle\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.x-dev"
        }
    }
}
